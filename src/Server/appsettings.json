{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Debug",
        "System": "Debug"
      }
    },
    "WriteTo": [
      { "Name": "Console" }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "Documentive"
    }
  },
  "ConnectionStrings": {
    "ConnectionString": "Server=.\\Sql2019;Database=OpenDoko;Trusted_Connection=True;"
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "OpenDoko": {
    "NumberOfTables": 4
  },
  "HealthChecksUI": {
    "HealthChecks": [
      {
        "Name": "OpenDoko Server",
        "Uri": "/healthz"
      }
    ],
    "Webhooks": [],
    "EvaluationTimeinSeconds": 10,
    "MinimumSecondsBetweenFailureNotifications": 60
  }
}